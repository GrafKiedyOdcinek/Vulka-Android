{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Librus API '/Timetable' endpoint, only date",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "Lesson": {
        "type": "object",
        "properties": {
          "Id": {
            "type": "string"
          },
          "Url": {
            "type": "string"
          }
        },
        "required": [
          "Id",
          "Url"
        ]
      },
      "Classroom": {
        "type": "object",
        "properties": {
          "Id": {
            "type": "string"
          },
          "Url": {
            "type": "string"
          }
        },
        "required": [
          "Id",
          "Url"
        ]
      },
      "DateFrom": {
        "type": "string"
      },
      "DateTo": {
        "type": "string"
      },
      "LessonNo": {
        "type": "string"
      },
      "TimetableEntry": {
        "type": "object",
        "properties": {
          "Id": {
            "type": "string"
          },
          "Url": {
            "type": "string"
          }
        },
        "required": [
          "Id",
          "Url"
        ]
      },
      "DayNo": {
        "type": "string"
      },
      "Subject": {
        "type": "object",
        "properties": {
          "Id": {
            "type": "string"
          },
          "Name": {
            "type": "string"
          },
          "Short": {
            "type": "string"
          },
          "Url": {
            "type": "string"
          }
        },
        "required": [
          "Id",
          "Name",
          "Short",
          "Url"
        ]
      },
      "Teacher": {
        "type": "object",
        "properties": {
          "Id": {
            "type": "string"
          },
          "FirstName": {
            "type": "string"
          },
          "LastName": {
            "type": "string"
          },
          "Url": {
            "type": "string"
          }
        },
        "required": [
          "Id",
          "FirstName",
          "LastName",
          "Url"
        ]
      },
      "IsSubstitutionClass": {
        "type": "boolean"
      },
      "IsCanceled": {
        "type": "boolean"
      },
      "SubstitutionNote": {},
      "HourFrom": {
        "type": "string"
      },
      "HourTo": {
        "type": "string"
      },
      "VirtualClass": {
        "type": "object",
        "properties": {
          "Id": {
            "type": "integer"
          },
          "Url": {
            "type": "string"
          }
        },
        "required": [
          "Id",
          "Url"
        ]
      },
      "VirtualClassName": {
        "type": "string"
      },
      "Class": {
        "type": "object",
        "properties": {
          "Id": {
            "type": "integer"
          },
          "Url": {
            "type": "string"
          }
        },
        "required": [
          "Id",
          "Url"
        ]
      }
    },
    "required": [
      "Lesson",
      "DateFrom",
      "DateTo",
      "LessonNo",
      "TimetableEntry",
      "DayNo",
      "Subject",
      "Teacher",
      "IsSubstitutionClass",
      "IsCanceled",
      "SubstitutionNote",
      "HourFrom",
      "HourTo"
    ]
  }
}
