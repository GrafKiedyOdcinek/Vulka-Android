            "UserId": {
              "type": "integer"
            },
            "FirstName": {
              "type": "string"
            },
            "LastName": {
              "type": "string"
            },
            "Email": {
              "type": "string"
            },
            "GroupId": {
              "type": "integer"
            },
            "IsActive": {
              "type": "boolean"
            },
            "Login": {
              "type": "string"
            },
            "IsPremium": {
              "type": "boolean"
            },
            "IsPremiumDemo": {
              "type": "boolean"
            },
            "ExpiredPremiumDate": {
              "type": "integer"
            },
            "PremiumAddons": {
              "type": "array",
